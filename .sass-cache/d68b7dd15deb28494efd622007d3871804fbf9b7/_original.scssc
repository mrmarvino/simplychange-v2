.nav-next {
  float: right;
  text-align: right;
  width: 50%;
}
#nav-above {
  margin: 0 0 18px 0;
}
#nav-above {
  display: none;
}
.paged #nav-above,
.single #nav-above {
  display: block;
}
#nav-below {
  margin: -18px 0 0 0;
}


/* =Comments
-------------------------------------------------------------- */
#comments {
  clear: both;
}
#comments .navigation {
  padding: 0 0 18px 0;
}
h3#comments-title,
h3#reply-title {
  color: #666;
  font-size: 20px;
  font-weight: normal;
  margin-bottom: 0;
}
h3#comments-title {
  padding: 24px 0;
}
.commentlist {
  list-style: none;
  margin: 0;
}
.commentlist li.comment {
  border-bottom: 1px solid #e7e7e7;
  line-height: 24px;
  margin: 0 0 24px 0;
  padding: 0 0 0 56px;
  position: relative;
}
.commentlist li:last-child {
  border-bottom: none;
  margin-bottom: 0;
}
#comments .comment-body ul,
#comments .comment-body ol {
  margin-bottom: 18px;
}
#comments .comment-body p:last-child {
  margin-bottom: 6px;
}
#comments .comment-body blockquote p:last-child {
  margin-bottom: 24px;
}
.commentlist ol {
  list-style: decimal;
}
.commentlist .avatar {
  position: absolute;
  top: 4px;
  left: 0;
}
.comment-author {
}
.comment-author cite {
  color: #000;
  font-style: normal;
  font-weight: bold;
}
.comment-author .says {
  font-style: italic;
}
.comment-meta {
  font-size: 12px;
  margin: 0 0 18px 0;
}
.comment-meta a:link,
.comment-meta a:visited {
  color: #888;
  text-decoration: none;
}
.comment-meta a:active,
.comment-meta a:hover {
  color: #ff4b33;
}
.commentlist .even {
}
.commentlist .bypostauthor {
}
.reply {
  font-size: 12px;
  padding: 0 0 24px 0;
}
.reply a,
a.comment-edit-link {
  color: #888;
}
.reply a:hover,
a.comment-edit-link:hover {
  color: #ff4b33;
}
.commentlist .children {
  list-style: none;
  margin: 0;
}
.commentlist .children li {
  border: none;
  margin: 0;
}
.nopassword,
.nocomments {
  display: none;
}
#comments .pingback {
  border-bottom: 1px solid #e7e7e7;
  margin-bottom: 18px;
  padding-bottom: 18px;
}
.commentlist li.comment+li.pingback {
  margin-top: -6px;
}
#comments .pingback p {
  color: #888;
  display: block;
  font-size: 12px;
  line-height: 18px;
  margin: 0;
}
#comments .pingback .url {
  font-size: 13px;
  font-style: italic;
}

/* Comments form */
input[type=submit] {
  color: #333;
}
#respond {
  border-top: 1px solid #e7e7e7;
  margin: 24px 0;
  overflow: hidden;
  position: relative;
}
#respond p {
  margin: 0;
}
#respond .comment-notes {
  margin-bottom: 1em;
}
.form-allowed-tags {
  line-height: 1em;
}
.children #respond {
  margin: 0 48px 0 0;
}
h3#reply-title {
  margin: 18px 0;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
  
}
#comments-list #respond {
  margin: 0 0 18px 0;
}
#comments-list ul #respond {
  margin: 0;
}
#cancel-comment-reply-link {
  font-size: 12px;
  font-weight: normal;
  line-height: 18px;
}
#respond .required {
  color: #ff4b33;
  font-weight: bold;
}
#respond label {
  color: #888;
  font-size: 12px;
}
#respond input {
  margin: 0 0 9px;
  width: 98%;
}
#respond textarea {
  width: 98%;
}
#respond .form-allowed-tags {
  color: #888;
  font-size: 12px;
  line-height: 18px;
}
#respond .form-allowed-tags code {
  font-size: 11px;
}
#respond .form-submit {
  margin: 12px 0;
}
#respond .form-submit input {
  font-size: 14px;
  width: auto;
}


/* =Widget Areas
-------------------------------------------------------------- */

.widget-area ul {
  list-style: none;
  margin-left: 0;
}
.widget-area ul ul {
  list-style: square;
  margin-left: 1.3em;
}
.widget-area select {
  max-width: 100%;
}
.widget_search #s {/* This keeps the search inputs in line */
  width: 60%;
}
.widget_search label {
  display: none;
}
.widget-container {
  margin: 0 0 18px 0;
}
.widget-title {
  color: #222;
  font-weight: normal;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
  font-size: 18px;
  line-height: 28px;
}
.widget-area a:link,
.widget-area a:visited {
  text-decoration: none;
}
.widget-area a:active,
.widget-area a:hover {
  text-decoration: underline;
}
.widget-area .entry-meta {
  font-size: 11px;
}
#wp_tag_cloud div {
  line-height: 1.6em;
}
#wp-calendar {
  width: 100%;
}
#wp-calendar caption {
  color: #222;
  font-size: 14px;
  font-weight: bold;
  padding-bottom: 4px;
  text-align: left;
}
#wp-calendar thead {
  font-size: 11px;
}
#wp-calendar thead th {
}
#wp-calendar tbody {
  color: #aaa;
}
#wp-calendar tbody td {
  background: #f5f5f5;
  border: 1px solid #fff;
  padding: 3px 0 2px;
  text-align: center;
}
#wp-calendar tbody .pad {
  background: none;
}
#wp-calendar tfoot #next {
  text-align: right;
}
.widget_rss a.rsswidget {
  color: #000;
}
.widget_rss a.rsswidget:hover {
  color: #ff4b33;
}
.widget_rss .widget-title img {
  width: 11px;
  height: 11px;
}

/* Main sidebars */
#main .widget-area ul {
  margin-left: 0;
  padding: 0 20px 0 0;
}
#main .widget-area ul ul {
  border: none;
  margin-left: 1.3em;
  padding: 0;
}
#primary {
  background: #D3EAF3 url(../images/features-panel.png) repeat-x;
  border-radius: 5px;
  margin: 10px 0;
}
#secondary {
}

/* Footer widget areas */
#footer-widget-area {
}


/* =Footer
-------------------------------------------------------------- */

#footer {
  margin-top:  10px;
  margin-bottom: 0;
  background: #e9e9e9 url(../images/footer-top.png) no-repeat;
  height:  100px;
}
#colophon {
  margin-top: -4px;
  overflow: hidden;
  padding: 38px 0;
}
#site-info {
  font-weight: normal;
  padding-left: 20px;
  width: 920px;
}

#site-info ul{
  list-style: none;
  padding-left: 0;
  margin: 0;
}

#site-info ul li{
  display: inline;
  padding-right: 5px;
  padding-left: 5px;
  border-right: 1px solid #006892;;
}
#site-info ul li.first{
  padding-left: 0;
}
#site-info ul li.last{
  border: none;
  padding-right: 0;
}
#site-info a {
  color: #006892;
  text-decoration: none;
}
#site-generator {
  font-style: italic;
  position: relative;
}
#site-generator a {
  background: url(../images/wordpress.png) center left no-repeat;
  color: #666;
  display: inline-block;
  line-height: 16px;
  padding-left: 20px;
  text-decoration: none;
}
#site-generator a:hover {
  text-decoration: underline;
}
img#wpstats {
  display: block;
  margin: 0 auto 10px;
}


/* =Mobile Safari ( iPad, iPhone and iPod Touch )
-------------------------------------------------------------- */

pre {
  -webkit-text-size-adjust: 140%;
}
code {
  -webkit-text-size-adjust: 160%;
}
#access,
.entry-meta,
.entry-utility,
.navigation,
.widget-area {
  -webkit-text-size-adjust: none;
}
body,
#site-description {
  -webkit-text-size-adjust: none;
}


/* =Print Style
-------------------------------------------------------------- */

@media print {
  body {
    background: none !important;
  }
  #wrapper {
    clear: both !important;
    display: block !important;
    float: none !important;
    position: relative !important;
  }
  #header {
    border-bottom: 2pt solid #000;
    padding-bottom: 18pt;
  }
  #colophon {
    border-top: 2pt solid #000;
  }
  #site-title,
  #site-description {
    float: none;
    line-height: 1.4em;
    margin: 0;
    padding: 0;
  }
  #site-title {
    font-size: 13pt;
  }
  .entry-content {
    font-size: 14pt;
    line-height: 1.6em;
  }
  .entry-title {
    font-size: 21pt;
  }
  #access,
  #branding img,
  #respond,
  .comment-edit-link,
  .edit-link,
  .navigation,
  .page-link,
  .widget-area {
    display: none !important;
  }
  #container,
  #header,
  #footer {
    margin: 0;
    width: 100%;
  }
  #content,
  .one-column #content {
    margin: 24pt 0 0;
    width: 100%;
  }
  .wp-caption p {
    font-size: 11pt;
  }
  #site-info,
  #site-generator {
    float: none;
    width: auto;
  }
  #colophon {
    width: auto;
  }
  img#wpstats {
    display: none;
  }
  #site-generator a {
    margin: 0;
    padding: 0;
  }
  #entry-author-info {
    border: 1px solid #e7e7e7;
  }
  #main {
    display: inline;
  }
  .home .sticky {
    border: none;
  }
}

/* =Custom styles
-------------------------------------------------------------- */

/* search */
input#searchsubmit{
  background: url(../images/search-btn.png) 0 0px no-repeat;
  text-indent: -2000px;
  width: 39px;
  height: 25px;
  border: none;
  margin: 2px 0 0 -30px;
  cursor: pointer;
}

input#searchsubmit:hover{
  background: url(../images/search-btn-over.png) 0 0px no-repeat;
}

input#s{
  width: 170px;
  height: 21px;
  padding: 2px 30px 0px 0;
}


/* intro panel*/
.intro-panel{
  background: url(../images/intro-panel.png) repeat-x;
  padding: 10px 10px 0 10px;
  overflow: auto;
}

.intro-headers{
  width: 190px;
  float: left;
  padding: 5px 0 0 10px;
}

.intro-headers p{
  padding: 5px 0 0 7px;
}

.intro-panel h1{
  font-size: 42px;
  color: #006792;
  line-height: 36px;
  text-shadow: 1px 1px 1px #fff;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
}

.intro-panel p strong{
  color: #006792;
  font-weight: normal;
  
}

.intro-panel em{
  font-size: 18px;
  color: #006792;
  font-style: normal;
  
  
}

.intro-body{
  float: left;
  margin-left: 20px;
  width: 420px;
  font-size: 16px;
  padding: 10px 290px 5px 10px;
  background:  url(../images/home-image.png) 355px -25px no-repeat;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
}

.intro-body p{
  line-height: 22px;
}

.features-panel{
  background:  url(../images/intro-panel-bottom.png) repeat-x;
}

.features-content{
  background: #d3eaf3 url(../images/features-panel.png) repeat-x;
  padding: 0 10px 20px 20px;
}

.features-content h2{
  font-size: 30px;
  line-height: 30px;
  padding: 10px 0;
  text-shadow: 1px 1px 1px #fff;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
}

.home-content{
  background: #fff; 
  float: left; 
  width: 640px; 
  margin: 10px 10px 20px 10px;
}

#content .home-content h2{
  color: #666;
  font-size: 30px;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;
}

#content .home-content h3{
  font-size: 16px;
  margin: 0 0 5px 0;
  line-height: 1.2em;
}

#content .home-content h3 a{
  text-decoration: none;
}

#content .home-content h3 a:hover{
  text-decoration: underline;
}

.home-content .col-1{
  float: left;
  width: 280px;
  padding: 10px;
  margin-right: 20px;
}

.home-content .col-2{
  float: left;
  width: 280px;
  padding: 10px;
}

#content .home-content a.read-more{
  display: inline-block;
  background: #006792;
  color: #fff;
  text-decoration: none;
  padding: 0 5px;
  border-radius: 3px;
}
#content .home-content a.read-more:hover{
  background: #6DCBD8;  
}


/* Tiny Carousel */
#slider1 { height: 100px; overflow:hidden; padding: 0 0 10px; }
#slider1 .viewport { float: left; width: 599px; height: 125px; overflow: hidden; position: relative; }
#slider1 .buttons { background: url(../images/arrow-left.png) no-repeat; display: block; margin: 30px 0 0 0; background-position: 0 0; text-indent: -999em; float: left; width: 10px; height: 37px; overflow: hidden; position: relative;}
#slider1 .next { background-position: 0 0; margin: 30px 0 0 0; background: url(../images/arrow-right.png) no-repeat;}
#slider1 .disable { visibility: hidden; }
#slider1 .overview { list-style: none; position: absolute; padding: 0; margin: 0; width: 240px; left: 0; top: 0; }

#slider1 .overview li{ float: left; margin: 0 20px 0 0; padding: 1px; height: 96px; border: 3px solid #fff; width: 178px; background: #fff; /*background: #fff url(../images/scroller-bg-1.png) repeat-x;*/}

#slider1 .overview li h3{
  padding: 5px 0 0 8px;
  line-height: 14px
}

#slider1 .overview li h3 a{
  font-weight: normal;
  text-decoration: none;
}

#slider1 .overview li:hover{
  background: #fff;
  border-color: #006792;
}

#slider1 .overview li p{
  padding: 8px;
  line-height: 14px;
}

#slider1 li div{
  padding-right: 40px;
  min-height: 98px;
}

#facilitation div{
  background: url(../images/scroller-facilitation-bg.png) right top no-repeat;
}

#prog-manage div{
  background: url(../images/scroller-prog-manage-bg.png) 130px top no-repeat;
}

#change-management div{
  background: url(../images/scroller-change-manage-bg.png) 110px top no-repeat;
}

#embedding-change div{
  background: url(../images/scroller-exe-change.png) right top no-repeat;
}

/*Twitter Feed*/

.feed-container{
  float: left;
  width: 290px;
  margin: -160px 0 0 0; 
}

.twitter-feed{
  background: url(../images/twitter-border-top.png) no-repeat; 
  padding: 5px 0 0 0; 
  font-size: 12px;
  line-height: 16px;
  zoom:1;
}

#content .twitter-feed h2{
  color: #fff;
  padding: 5px 0 0 10px;
  margin-bottom: 10px;
  font-family: "Museo300", Arial, Helvetica, "Nimbus Sans L", sans-serif;

}

#tweet{
  padding: 10px;
  min-height: 200px;
}

#tweet img.ajax-loader{
  text-align: center;
  margin: 50px 0 0 120px;
}

#tweet p{
  padding: 0 35px 0 35px
}

#tweet ul{
  margin: 0 10px 0 43px;
  list-style: none;
}

#tweet ul li{
  padding: 0 0 15px 0;
}

#tweet .prefix img{
  margin-left: -38px;
  position: absolute;
}

.twitter-footer{
  background: #fff url(../images/twitter-border-bottom.png) left bottom no-repeat;
  height: 50px;
}

.twitter-footer p{
  padding: 0 47px 30px 47px;
}

.feed-container .follow-us{
  list-style: none;
}

#content .feed-container h3{
  display: block;
  color: #666;
  text-align: right;
  width: 160px;
  float: left;
  padding-top: 5px;
}

.feed-container .follow-us{
  display: block;
  float: right;
  width: 130px;
  margin: 1px 0 0 0;
}

.feed-container .follow-us li{
  float: right;
}

.feed-container .twitter,
.feed-container .facebook,
.feed-container .linkedin,
.feed-container .rss{
  display: block;
  width: 25px;
  height: 25px;
  text-indent: -2000px;
  margin-right: 5px;
}

.feed-container .twitter{
  background: url(../images/icon-twitter.png) no-repeat;
}

.feed-container .facebook{
  background: url(../images/icon-facebook.png) no-repeat;
}

.feed-container .linkedin{
  background: url(../images/icon-linkedin.png) no-repeat;
}

.feed-container .rss{
  background: url(../images/icon-rss.png) no-repeat;
}


